    "image-webpack-loader": "^3.1.0",
    "less-loader": "^2.2.3",
    "postcss-loader": "^1.2.1",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-filterable-log-monitor": "^0.6.6",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
